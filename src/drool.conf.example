log all;
nolog network debug;

read "/path/to/file.pcap";
#input "eth0";

filter "udp port 53";

timing ignore;
#timing keep;
#timing add 100;
#timing reduce 200;
#timing multiply 1.5;

client_pool target 127.0.0.1 53;
client_pool max_clients 5000;
client_pool client_ttl 0.05;

#write "/path/to/file.pcap";
#output "eth1";

